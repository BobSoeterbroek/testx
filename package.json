{
  "name": "testx",
  "version": "3.0.0",
  "description": "A library for executing keyword driven tests with Protractor.",
  "main": "lib/index.js",
  "scripts": {
    "build": "coffee  -o lib/ -c src/",
    "prepublish": "npm run build",
    "pretest": "webdriver-manager update; npm run build",
